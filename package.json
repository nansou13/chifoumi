{
  "name": "react-redux-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.1",
    "prop-types": "^15.7.2",
    "react": "^16.8.4",
    "react-dom": "^16.8.4",
    "react-redux": "^6.0.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.8",
    "redux": "^4.0.1",
    "styled-components": "^4.1.3"
  },
  "scripts": {
    "start": "REACT_APP_ENV_TYPE=development react-scripts start",
    "start:preprod": "REACT_APP_ENV_TYPE=preprod react-scripts start",
    "start:prod": "REACT_APP_ENV_TYPE=production react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint:js": "eslint . --ext .js,.jsx",
    "lint:js:fix": "npm run lint:js -- --fix",
    "lint": "npm run lint:js",
    "lint:staged": "lint-staged",
    "start:mock": "node src/utils/mock/server.js",
    "start:all": "run-p start:mock start"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "pre-commit": "lint:staged",
  "lint-staged": {
    "*.js": "npm run lint"
  },
  "babel": {
    "presets": [
      "env",
      "react-app",
      "stage-0"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "displayName": true
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.3.4",
    "@babel/preset-env": "^7.3.4",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/runtime": "^7.3.4",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "^8.0.5",
    "cors": "^2.8.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.11.2",
    "eslint": "5.12.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^4.1.0",
    "eslint-config-react-app": "^3.0.8",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-flowtype": "^3.4.2",
    "eslint-plugin-import": "^2.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "jest": "23.6.0",
    "lint-staged": "^8.1.5",
    "lowdb": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.4",
    "promise": "^8.0.2"
  }
}
